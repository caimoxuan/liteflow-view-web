{
  "private": true,
  "scripts": {
    "copy-files": "copyfiles -u 1 \"src/**/*.less\" es/src",
    "dev": "umi dev",
    "start": "export NODE_OPTIONS=--openssl-legacy-provider && umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "authors": [
    "shijiangjiayou@163.com", "wu_qingwan@126.com"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.6",
    "@antv/layout": "^0.1.31",
    "@antv/x6": "^1.7.12",
    "@antv/x6-react-shape": "^1.6.0",
    "antd": "^4.5.3",
    "react": "17.x",
    "react-dom": "17.x",
    "react-json-view": "^1.19.1",
    "react-split-pane-v2": "^1.0.3",
    "umi": "^3.5.20"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.20",
    "copyfiles": "^2.4.1",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
